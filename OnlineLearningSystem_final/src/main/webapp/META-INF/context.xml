			  auth="Container"
			  type="javax.sql.DataSource"
			  maxTotal="20"
			  maxIdle="10"
			  maxWaitMillis="-1"
			  username="root"
			  password="password"
			  driverClassName="com.mysql.cj.jdbc.Driver"
			  url="jdbc:mysql://localhost:3306/userdb"/>
</Context>